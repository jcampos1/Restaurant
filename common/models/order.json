{
  "name": "order",
  "plural": "orders",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "orderdate": {
      "type": "date"
    },
    "total": {
      "type": "number",
      "required": true
    },
    "boardnumb": {
      "type": "string",
      "required": true
    },
    "active": {
      "type": "boolean",
      "required": true,
      "default": true
    }
  },
  "validations": [],
  "relations": {
    "items": {
      "type": "hasMany",
      "model": "item",
      "foreignKey": "orderId"
    },
    "board": {
      "type": "belongsTo",
      "model": "Board",
      "foreignKey": "boardId"
    },
    "user": {
      "type": "belongsTo",
      "model": "user",
      "foreignKey": "userId"
    }
  },
  "acls": [],
  "methods": {}
}
